@article{Reid1989,
  title = {The Distribution of Species of the Genus {{Thermocyclops}} ({{Copepoda}}, {{Cyclopoida}}) in the Western Hemisphere, with Description of {{T}}. Parvus, New Species},
  author = {Reid, Janet W.},
  date = {1989-05-01},
  journaltitle = {Hydrobiologia},
  shortjournal = {Hydrobiologia},
  volume = {175},
