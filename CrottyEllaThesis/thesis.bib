  author = {Karstensen, Johannes and Stramma, Lothar and Visbeck, Martin},
  date = {2008-06-01},
  journaltitle = {Progress in Oceanography},
  shortjournal = {Progress in Oceanography},
  series = {A {{New View}} of {{Water Masses After WOCE}}. {{A Special Edition}} for {{Professor Matthias Tomczak}}},
  volume = {77},
