@article{Elliott2012,
  title = {Relationship between Environmental Conditions and Zooplankton Community Structure during Summer Hypoxia in the Northern {{Gulf}} of {{Mexico}}},
  author = {Elliott, David T. and Pierson, James J. and Roman, Michael R.},
  date = {2012-07-01},
  journaltitle = {Journal of Plankton Research},
  shortjournal = {Journal of Plankton Research},
