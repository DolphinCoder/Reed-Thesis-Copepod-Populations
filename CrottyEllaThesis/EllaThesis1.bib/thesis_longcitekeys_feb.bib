	note = {Publisher: Proceedings of the National Academy of Sciences},
	keywords = {Hypoxia},
	file = {Full Text PDF:/Users/ellac/Zotero/storage/7ABBKACD/Keeling and Garcia - 2002 - The change in oceanic O2 inventory associated with.pdf:application/pdf},
}

@article{deutsch_climate-forced_2011,
	title = {Climate-Forced Variability of Ocean Hypoxia},
	volume = {333},
	url = {https://www.science.org/doi/10.1126/science.1202422},
	doi = {10.1126/science.1202422},
	abstract = {Oxygen (O2) is a critical constraint on marine ecosystems. As oceanic O2 falls to hypoxic concentrations, habitability for aerobic organisms decreases rapidly. We show that the spatial extent of hypoxia is highly sensitive to small changes in the ocean’s O2 content, with maximum responses at suboxic concentrations where anaerobic metabolisms predominate. In model-based reconstructions of historical oxygen changes, the world’s largest suboxic zone, in the Pacific Ocean, varies in size by a factor of 2. This is attributable to climate-driven changes in the depth of the tropical and subtropical thermocline that have multiplicative effects on respiration rates in low-O2 water. The same mechanism yields even larger fluctuations in the rate of nitrogen removal by denitrification, creating a link between decadal climate oscillations and the nutrient limitation of marine photosynthesis.},
	pages = {336--339},
	number = {6040},
	journaltitle = {Science},
	author = {Deutsch, Curtis and Brix, Holger and Ito, Taka and Frenzel, Hartmut and Thompson, {LuAnne}},
	urldate = {2025-02-11},
	date = {2011-07-15},
	note = {Publisher: American Association for the Advancement of Science},
	keywords = {Hypoxia},
	file = {Full Text PDF:/Users/ellac/Zotero/storage/W5PIWLFM/Deutsch et al. - 2011 - Climate-Forced Variability of Ocean Hypoxia.pdf:application/pdf},
}

@article{ito_conceptual_2010,
