	note = {Publisher: [American Institute of Biological Sciences, Oxford University Press]},
	keywords = {Hypoxia},
	file = {JSTOR Full Text PDF:/Users/ellac/Zotero/storage/EKLY8IPA/Somero et al. - 2016 - What Changes in the Carbonate System, Oxygen, and .pdf:application/pdf},
}

