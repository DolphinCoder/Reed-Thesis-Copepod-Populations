	note = {Publisher: American Association for the Advancement of Science},
	keywords = {Hypoxia},
	file = {Full Text PDF:/Users/ellac/Zotero/storage/WWKKQD8F/Howard et al. - 2020 - Climate-driven aerobic habitat loss in the Califor.pdf:application/pdf},
}

