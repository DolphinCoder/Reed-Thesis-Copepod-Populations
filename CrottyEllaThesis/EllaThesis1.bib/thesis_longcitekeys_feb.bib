	langid = {english},
	note = {Publisher: Nature Publishing Group},
	keywords = {Temperature, Stratification},
	file = {Full Text PDF:/Users/ellac/Zotero/storage/XYITWXUB/Li et al. - 2020 - Increasing ocean stratification over the past half.pdf:application/pdf},
}

