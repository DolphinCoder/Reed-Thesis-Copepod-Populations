	doi = {10.1016/B978-0-08-095975-7.00603-3},
	keywords = {Hypoxia},
	file = {ScienceDirect Snapshot:/Users/ellac/Zotero/storage/R9YIUCL2/B9780080959757006033.html:text/html},
}

